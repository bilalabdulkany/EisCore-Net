{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Network",
      "Serilog.Sinks.Http"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp} {Level} {Message} {NewLine:1} {Exception:1}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "/Logs/log.txt",
          "outputTemplate": "{Timestamp} {Level} {Message} {NewLine:1} {Exception:1}",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "environment": {
    "profile": "development"
  },
  "AllowedHosts": "*",
  "DatabaseSource": "Data Source=TestCore.sqlite",
  "eis": {
    "source-system-name": "MDM"
  }
}